{
  "name": "@sito/dashboard-app",
  "private": false,
  "version": "0.0.47",
  "type": "module",
  "description": "UI Library with prefab components",
  "main": "dist/dashboard-app.cjs",
  "module": "dist/dashboard-app.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/"
  ],
  "homepage": "https://github.com/sito8943/-sito-dashboard-app#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/sito8943/-sito-dashboard-app.git"
  },
  "bugs": {
    "url": "https://github.com/sito8943/-sito-dashboard-app/issues"
  },
  "exports": {
    ".": {
      "import": "./dist/dashboard-app.js",
      "require": "./dist/dashboard-app.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "sideEffects": [
    "**/*.css"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,scss,md}\""
  },
  "keywords": [
    "sito",
    "dashboard",
    "components"
  ],
  "author": "sito8943",
  "license": "ISC",
  "peerDependencies": {
    "@emotion/css": "11.13.5",
    "react": "18.3.1",
    "@sito/dashboard": "0.0.47"
  },
  "devDependencies": {
    "@types/node": "20.8.10",
    "@types/react": "18.3.3",
    "@types/react-dom": "18.2.14",
    "@typescript-eslint/eslint-plugin": "6.0.0",
    "@typescript-eslint/parser": "6.0.0",
    "@vitejs/plugin-react": "4.0.3",
    "autoprefixer": "10.4.16",
    "eslint": "8.45.0",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-react-refresh": "0.4.3",
    "postcss": "8.4.41",
    "prettier": "3.3.3",
    "tailwindcss": "3.4.10",
    "typescript": "5.5.4",
    "vite": "4.2.0",
    "vite-plugin-dts": "4.0.2",
    "vite-plugin-lib-inject-css": "1.3.0"
  },
  "dependencies": {}
}
